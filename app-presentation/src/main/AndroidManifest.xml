<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="cat.santi.ttfe.presentation"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".Presentation"
        android:allowBackup="true"
        android:icon="@mipmap/ttfe__ic_launcher"
        android:label="@string/ttfe__app_name"
        android:theme="@style/TTFE__AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/ttfe__app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!--<meta-data-->
            <!--android:name="io.fabric.ApiKey"-->
            <!--android:value="6eabce271dd827b58c0d2e54143873ec09bcb90e"/>-->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
    </application>
</manifest>